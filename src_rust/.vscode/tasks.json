{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cargo run",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "cargo run --bin=verflist_puzzle"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "shared",
                "clear": false
            },
            "problemMatcher": ["$rustc"],
            "options": {
                "cwd": "${workspaceFolder}"
            }
        },
                {
            "label": "cargo Build",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "cargo build --bin=verflist_puzzle"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "shared",
                "clear": false
            },
            "problemMatcher": ["$rustc"],
            "options": {
                "cwd": "${workspaceFolder}"
            }
        }
    ]
}